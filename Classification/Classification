    "    y_predNN3ai = cross_val_predict(NN3a, data3a, target3a, cv=10)\n",
    "    confusNN3ai = confusion_matrix(target3a, y_predNN3ai)\n",
    "    \n",
    "    print('\\033[1m'+'\\033[4m')\n",
    "    print(\"results for k = {} \".format(i))\n",
    "    print('\\033[0m')\n",
    "    print('\\033[1m')\n",
    "    print('CV Mean:')\n",
    "    print('\\033[0m')\n",
    "    print(round(np.mean(cv_scoresNN3ai),3))\n",
